{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "boilerplate",
  "engines": {
    "npm": ">=3",
    "node": ">=8"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "node_modules/.bin/mocha",
    "build": "babel src/ -d dist/",
    "flow": "node_modules/.bin/flow",
    "prepublish": "yarn run build",
    "production": "npm run flow && npm run build && grunt eslint && npm test && grunt jsdoc2md && grunt markdown_toc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ltodt@github.com/ltodt/node-boilerplate.git"
  },
  "author": "Lasse Todt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ltodt/node-boilerplate/issues"
  },
  "homepage": "https://github.com/ltodt/node-boilerplate#readme",
  "dependencies": {
    "dotenv": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.53.0",
    "grunt": "^1.0.1",
    "grunt-babel": "^7.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^20.0.0",
    "grunt-jsdoc-to-markdown": "^3.0.0",
    "grunt-markdown-toc": "^0.0.2",
    "mocha": "^3.5.0"
  }
}
